{
  "ConnectionStrings": {
    "PFire": "pfiredb.sqlite"
  },
  "ServerSettings": {
    "OpenSqlConnection": false
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Properties": {
      "Application": "PFire"
    }
  }
}